{
  "name": "@meplato/ui-kit",
  "version": "2.0.13",
  "type": "module",
  "types": "./index.d.ts",
  "description": "Reusable UI components for Meplato apps",
  "main": "./ui-kit.umd.js",
  "module": "./ui-kit.es.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./ui-kit.es.js",
      "require": "./ui-kit.umd.js"
    },
    "./ui-kit.css": {
      "import": "./ui-kit.css",
      "require": "./ui-kit.css"
    }
  },
  "style": "./ui-kit.css",
  "author": "Meplato UI Team <ui-team@meplato.com>",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/meplato/ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/meplato/ui-kit/issues"
  },
  "scripts": {
    "build": "tsc && vite build",
    "build-dev": "tsc && vite build && cp package.json dist",
    "test": "vitest",
    "storybook": "STORYBOOK_PREVIEW=1 storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.2",
    "@fontsource-variable/inter": "^5.0.15",
    "@fontsource/roboto-mono": "^5.0.15",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-radix": "^2.8.0",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-essentials": "^7.6.2",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.5.2",
    "@storybook/preview-api": "^7.6.2",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.6.2",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.9.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "mock-match-media": "^0.4.3",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.5.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
